<core:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m"
		controllerName="view.NoteItem" xmlns:html="http://www.w3.org/1999/xhtml">
	<Page id="NotePage" >
		<content>
				<ObjectHeader title="{Author}" > 
					<attributes>
						<ObjectAttribute text="{ 
							path:'CreatedAt', 
							formatter:'sap.ui.demo.myFiori.util.Formatter.date' 
						}" /> 
					</attributes> 
				</ObjectHeader>		   
		</content>
	</Page>
</core:View>